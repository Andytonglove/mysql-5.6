--enable_binlog_hlc=true --maintain_database_hlc=true
--allow_noncurrent_db_rw=off --enable_block_stale_hlc_read=true
--session-track-response-attributes=ON --response_attrs_contain_hlc=true
--rocksdb_enable_udt_in_mem=true --enable_exactly_at_hlc=true
--rocksdb_default_cf_options=write_buffer_size=8m;target_file_size_base=1m;max_bytes_for_level_base=1m;compression_per_level=kNoCompression;max_compaction_bytes=2m --default-dd-system-storage-engine=InnoDB
